
type Stanza {
  id: ID!

  name: String

  description: String

  template: String

  content: String

  revert_template: String
  revert_content: String

  device_type: String

  # a stanza can be applied to a device
  device: Device

  updatedAt: Timestamp!
  createdAt: Timestamp!
  appliedAt: Timestamp!

}